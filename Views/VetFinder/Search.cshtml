@{
}
@model VeterinarianSearchViewModel

<h1>Search Results for "@Model.Query"</h1>

@if (Model.Results.Count == 0)
{
    <p>No results found.</p>
}
else
{
    <ul>
        @foreach (var result in Model.Results)
        {
            <li>
                <h2>@result.Name</h2>
                <p>@result.Vicinity</p>
            </li>
        }
    </ul>
}
